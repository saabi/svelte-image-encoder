{
  "name": "svelte-image-encoder",
  "version": "1.2.5",
  "description": "A <ImgEncoder> Svelte component for editing and compressing profile pictures before upload to a server",
  "svelte": "dist/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.umd.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
